<h1>{{title}}</h1>
<a href="/delieveries/map/" class="btn btn-info">Map Location</a>
<table class="table table-stripped table-hover" id="delieveryTable">
    <thead>
        <th>Delievery ID</th>
        <th>Project ID</th>
        <th>Project Address</th>
        <th>Details</th>
        <th></th>
    </thead>
    <tbody>
        {{#each delieveryData}}
        <tr>
            <td>{{this.delieveryId}}</td>
            <td>{{this.projectId}}</td>
            <td>{{this.projectOwnerAddres}}</td>
            <td>
                <a href="#" class="btn btn-secondary">Details</a>
                <a href="#" class="btn btn-info">Map Location</a>
            </td>
            <td>
                <a href="#" class="btn btn-info">Update</a>
                &nbsp;
                <a href="#" class="btn btn-danger">Delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>